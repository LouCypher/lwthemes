<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
                   xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <ShortName>Mozilla Themes</ShortName>
  <Description>Lightweight Themes</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image width="16" height="16">data:image/png;base64,</Image>
  <moz:SearchForm>https://addons.mozilla.org/themes/</moz:SearchForm>
  <Url type="text/html" method="GET" template="https://addons.mozilla.org/search/">
    <Param name="q" value="{searchTerms}"/>
    <Param name="cat" value="themes"/>
  </Url>
  <Url rel="self" method="GET" type="application/opensearchdescription+xml"
       template="https://raw.github.com/LouCypher/lwthemes/master/search-plugin.xml"></Url>
</OpenSearchDescription>
